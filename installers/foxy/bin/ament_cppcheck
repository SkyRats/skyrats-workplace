#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ament-cppcheck==0.9.6','console_scripts','ament_cppcheck'
__requires__ = 'ament-cppcheck==0.9.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ament-cppcheck==0.9.6', 'console_scripts', 'ament_cppcheck')()
    )
