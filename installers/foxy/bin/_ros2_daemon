#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ros2cli==0.9.11','console_scripts','_ros2_daemon'
__requires__ = 'ros2cli==0.9.11'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ros2cli==0.9.11', 'console_scripts', '_ros2_daemon')()
    )
